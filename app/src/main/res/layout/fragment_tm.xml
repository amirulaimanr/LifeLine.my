<?xml version="1.0" encoding="utf-8"?>
<FrameLayout xmlns:android="http://schemas.android.com/apk/res/android"
    xmlns:app="http://schemas.android.com/apk/res-auto"
    xmlns:tools="http://schemas.android.com/tools"
    android:layout_width="match_parent"
    android:layout_height="match_parent"
    tools:context=".tmFragment">


    <androidx.constraintlayout.widget.ConstraintLayout
        android:layout_width="match_parent"
        android:layout_height="match_parent">

        <ImageView
            android:id="@+id/local"
            android:layout_width="129dp"
            android:layout_height="103dp"
            android:layout_marginStart="8dp"
            android:layout_marginTop="24dp"
            android:src="@drawable/local"
            app:layout_constraintEnd_toEndOf="parent"
            app:layout_constraintHorizontal_bias="0.0"
            app:layout_constraintStart_toStartOf="parent"
            app:layout_constraintTop_toTopOf="parent" />

        <ImageView
            android:id="@+id/international"
            android:layout_width="129dp"
            android:layout_height="103dp"
            android:layout_marginTop="24dp"
            android:src="@drawable/international"
            app:layout_constraintEnd_toEndOf="parent"
            app:layout_constraintHorizontal_bias="0.0"
            app:layout_constraintStart_toEndOf="@+id/local"
            app:layout_constraintTop_toTopOf="parent" />

        <TextView
            android:id="@+id/textView10"
            android:layout_width="wrap_content"
            android:layout_height="wrap_content"
            android:text="Local lines"
            app:layout_constraintEnd_toEndOf="@+id/local"
            app:layout_constraintStart_toStartOf="@+id/local"
            app:layout_constraintTop_toBottomOf="@+id/local" />

        <TextView
            android:id="@+id/textView25"
            android:layout_width="wrap_content"
            android:layout_height="wrap_content"
            android:text="International lines"
            app:layout_constraintEnd_toEndOf="@+id/international"
            app:layout_constraintStart_toStartOf="@+id/international"
            app:layout_constraintTop_toBottomOf="@+id/international" />
    </androidx.constraintlayout.widget.ConstraintLayout>

</FrameLayout>
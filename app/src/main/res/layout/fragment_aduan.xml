<?xml version="1.0" encoding="utf-8"?>
<FrameLayout xmlns:android="http://schemas.android.com/apk/res/android"
    xmlns:app="http://schemas.android.com/apk/res-auto"
    xmlns:tools="http://schemas.android.com/tools"
    android:layout_width="match_parent"
    android:layout_height="match_parent"
    tools:context=".aduanFragment">


    <androidx.constraintlayout.widget.ConstraintLayout
        android:layout_width="match_parent"
        android:layout_height="match_parent">

        <ImageView
            android:id="@+id/biro"
            android:layout_width="129dp"
            android:layout_height="103dp"
            android:layout_marginTop="24dp"
            android:src="@drawable/biro"
            app:layout_constraintEnd_toEndOf="parent"
            app:layout_constraintHorizontal_bias="0.0"
            app:layout_constraintStart_toStartOf="parent"
            app:layout_constraintTop_toTopOf="parent" />

        <ImageView
            android:id="@+id/mca"
            android:layout_width="129dp"
            android:layout_height="103dp"
            android:layout_marginTop="16dp"
            android:src="@drawable/mca"
            app:layout_constraintEnd_toEndOf="parent"
            app:layout_constraintHorizontal_bias="0.0"
            app:layout_constraintStart_toStartOf="parent"
            app:layout_constraintTop_toBottomOf="@+id/biro" />

        <ImageView
            android:id="@+id/pengguna"
            android:layout_width="129dp"
            android:layout_height="103dp"
            android:layout_marginStart="5dp"
            android:layout_marginTop="24dp"
            android:layout_marginEnd="5dp"
            android:src="@drawable/ppim"
            app:layout_constraintEnd_toStartOf="@+id/fomca"
            app:layout_constraintStart_toEndOf="@+id/biro"
            app:layout_constraintTop_toTopOf="parent" />

        <ImageView
            android:id="@+id/fomca"
            android:layout_width="129dp"
            android:layout_height="103dp"
            android:layout_marginTop="24dp"
            android:src="@drawable/fomca"
            app:layout_constraintEnd_toEndOf="parent"
            app:layout_constraintHorizontal_bias="1.0"
            app:layout_constraintStart_toStartOf="parent"
            app:layout_constraintTop_toTopOf="parent" />

        <TextView
            android:id="@+id/textView26"
            android:layout_width="wrap_content"
            android:layout_height="wrap_content"
            android:text="Biro Pengaduan"
            app:layout_constraintEnd_toEndOf="@+id/biro"
            app:layout_constraintStart_toStartOf="@+id/biro"
            app:layout_constraintTop_toBottomOf="@+id/biro" />

        <TextView
            android:id="@+id/textView31"
            android:layout_width="wrap_content"
            android:layout_height="wrap_content"
            android:text="PPIM"
            app:layout_constraintEnd_toEndOf="@+id/pengguna"
            app:layout_constraintStart_toStartOf="@+id/pengguna"
            app:layout_constraintTop_toBottomOf="@+id/pengguna" />

        <TextView
            android:id="@+id/textView34"
            android:layout_width="wrap_content"
            android:layout_height="wrap_content"
            android:text="FOMCA"
            app:layout_constraintEnd_toEndOf="@+id/fomca"
            app:layout_constraintStart_toStartOf="@+id/fomca"
            app:layout_constraintTop_toBottomOf="@+id/fomca" />

        <TextView
            android:id="@+id/textView35"
            android:layout_width="wrap_content"
            android:layout_height="wrap_content"
            android:layout_marginTop="4dp"
            android:text="MCA Hotline"
            app:layout_constraintEnd_toEndOf="@+id/mca"
            app:layout_constraintStart_toStartOf="@+id/mca"
            app:layout_constraintTop_toBottomOf="@+id/mca" />
    </androidx.constraintlayout.widget.ConstraintLayout>

</FrameLayout>